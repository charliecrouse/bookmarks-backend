{
  "name": "bookmarks-backend",
  "version": "0.2.0",
  "description": "Backend for a bookmarks manager",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./build ./dist",
    "prebuild": "npm run clean",
    "build": "npm run gen && tsc && babel --verbose ./dist --out-dir ./build",
    "start": "node ./build/index.js",
    "debug": "ts-node-dev --respawn --debug --trace-warnings --inspect=0.0.0.0:9229 ./src/index.ts",
    "lint": "eslint --fix --ext js,ts ./src",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charliecrouse/bookmarks-backend.git"
  },
  "author": "Charlie Crouse <charlietcrouse@icloud.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/charliecrouse/bookmarks-backend/issues"
  },
  "homepage": "https://github.com/charliecrouse/bookmarks-backend#readme",
  "dependencies": {
    "config": "^3.3.6",
    "graphql": "^15.8.0",
    "mongodb": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-resolvers": "^2.4.2",
    "@types/config": "^0.0.40",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
