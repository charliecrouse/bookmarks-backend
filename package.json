{
  "name": "bookmarks-api",
  "author": "Charlie Crouse <charlietcrouse@icloud.com>",
  "description": "API to manage bookmarks",
  "repository": {
    "type": "git",
    "url": "git@github.com:charliecrouse/bookmarks-backend.git"
  },
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc && babel --verbose ./dist --out-dir ./build",
    "clean": "rimraf ./build ./dist",
    "dev": "nodemon",
    "lint": "eslint --fix --ext js,ts .",
    "start": "yarn node ./build/index.js",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "argon2": "^0.28.2",
    "async": "^3.2.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.25.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.9.0",
    "sequelize-typescript": "^2.1.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@types/async": "^3.2.3",
    "@types/bluebird": "^3.5.29",
    "@types/body-parser": "^1.19.1",
    "@types/compression": "^1.0.1",
    "@types/config": "^0.0.36",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/morgan": "^1.7.37",
    "@types/node": "^16.11.7",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@yarnpkg/sdks": "^2.5.1-rc.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "node-gyp": "^6.0.1",
    "nodemon": "^1.19.4",
    "pinst": ">=2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
