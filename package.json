{
  "name": "bookmarks-backend",
  "version": "0.2.0",
  "description": "Backend for a simple bookmarks manager",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "start": "TS_NODE_BASEURL=./dist node -r tsconfig-paths/register ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "debug": "nodemon --debug --trace-warnings --inspect=0.0.0.0:9229 ./src/index.ts",
    "lint": "eslint --fix --ext js,ts ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charliecrouse/bookmarks-backend.git"
  },
  "author": "Charlie Crouse <charlietcrouse@icloud.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/charliecrouse/bookmarks-backend/issues"
  },
  "homepage": "https://github.com/charliecrouse/bookmarks-backend#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "date-fns": "^2.27.0",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "joi": "^17.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.2.2",
    "morgan": "^1.10.0",
    "papr": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.12.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.40",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.0",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
